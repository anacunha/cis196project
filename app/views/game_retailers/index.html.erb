<h1>Game Retailers</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @game_retailers.each do |game_retailer| %>
  <tr>
    <td><%= link_to game_retailer.name, game_retailer.website, :target => '_blank' %></td>
  </tr>
<% end %>
</table>

<br />

<div class="form-actions">
  <%= link_to 'Add a new Game Retailer', new_game_retailer_path, class: 'btn btn-primary' %>
</div>